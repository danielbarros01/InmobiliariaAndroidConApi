        app:layout_constraintTop_toBottomOf="@+id/tvInquilinoContrato"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvFechaInicioContrato"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="60dp"
        android:gravity="center"
        android:text="06/02/2020"
        android:textColor="#313131"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvFechaFinalizacionContrato"
        app:layout_constraintEnd_toEndOf="@+id/tvFechaFinalizacionContrato"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvLabelInquilino"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvFechaFinalizacionContrato"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="56dp"
        android:gravity="center"
        android:text="06/02/2021"
        android:textColor="#313131"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvMontoAlquilerContrato"
        app:layout_constraintEnd_toEndOf="@+id/tvMontoAlquilerContrato"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaInicioContrato"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnVerPagos"
        android:layout_width="211dp"
        android:layout_height="60dp"
        android:layout_marginBottom="82dp"
        android:text="Ver Pagos"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInmuebleContrato" />

</androidx.constraintlayout.widget.ConstraintLayout>