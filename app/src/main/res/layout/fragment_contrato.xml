        app:layout_constraintEnd_toEndOf="@+id/tvLabelInquilino2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaInicioContrato"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvLabelInquilino4"
