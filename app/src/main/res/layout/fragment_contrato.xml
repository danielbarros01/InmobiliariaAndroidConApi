        android:gravity="center"
        android:text="Inmueble"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvInmuebleContrato"
        app:layout_constraintEnd_toEndOf="@+id/tvInmuebleContrato"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInquilinoContrato"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvLabelInquilino2"
        android:layout_width="211dp"
        android:layout_height="0dp"
        android:layout_marginBottom="590dp"
        android:background="@drawable/bordes_derecha_redondeado_plata"
