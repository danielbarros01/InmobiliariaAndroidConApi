<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.inmuebles.InmuebleFragment">

    <ImageView
        android:id="@+id/ivInmuebleDetalles"
        android:layout_width="0dp"
        android:layout_height="250dp"
        android:scaleType="fitXY"
        android:src="@drawable/backgroundlogin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvPrecioInmuebleDetalles"
        android:layout_width="361dp"
        android:layout_height="46dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="27dp"
        android:shadowColor="#000000"
        android:shadowDx="4"
        android:shadowDy="4"
        android:shadowRadius="4"
        android:text="$17000"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvDireccionInmuebleDetalles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvDireccionInmuebleDetalles"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="499dp"
        android:shadowColor="#000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="4"
        android:text="Salta 325"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPrecioInmuebleDetalles"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBoxDisponibilidadInmueble"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_marginStart="12dp"
        android:text="Disponible"
        android:textColor="#303030"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivInmuebleDetalles"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="CÃ³digo"
        android:textColor="#2C2B2B"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/tvCodigoInmueble"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:text="Ambientes"
        android:textColor="#2C2B2B"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/tvAmbientesInmueble"
        android:layout_width="77dp"
        android:layout_height="36dp"
        android:text="3"
        android:textColor="#515151"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/tvTipoInmueble"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="1dp"
        android:text="Casa"
        android:textColor="#515151"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvAmbientesInmueble"
        app:layout_constraintEnd_toEndOf="@+id/tvUsoInmueble"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/tvUsoInmueble"
        android:layout_width="134dp"
        android:layout_height="37dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="155dp"
        android:text="Particular"
        android:textColor="#515151"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp"
        android:text="Tipo"
        android:textColor="#2C2B2B"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCodigoInmueble" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Uso"
        android:textColor="#2C2B2B"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/tvUsoInmueble"
        app:layout_constraintStart_toStartOf="@+id/tvUsoInmueble" />

    <TextView
        android:id="@+id/tvCodigoInmueble"
        android:layout_width="77dp"
        android:layout_height="36dp"
        android:text="503"
        android:textColor="#515151"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>